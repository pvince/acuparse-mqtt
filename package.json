{
  "name": "acuparse-mqtt",
  "version": "1.0.0",
  "description": "Relays Acurite data to an MQTT broker",
  "main": "dist/app.js",
  "scripts": {
    "build": "tsc",
    "start": "node --tls-min-v1.0 -r ts-node/register src/app.ts",
    "schema": "npx typescript-json-schema --esModuleInterop --required ./src/services/configuration.ts  IConfigurationJson > ./src/services/configuration.schema.json"
  },
  "keywords": [
    "acuparse",
    "mqtt",
    "acurite",
    "homeassistant",
    "hass"
  ],
  "author": "Paul Vincent",
  "license": "MIT",
  "devDependencies": {
    "@types/compression": "^1.7.2",
    "@types/debug": "^4.1.7",
    "@types/express": "^4.17.13",
    "@types/fs-extra": "^9.0.13",
    "@types/lodash": "^4.14.184",
    "@types/node": "^16.11.49",
    "@types/selfsigned": "^2.0.1",
    "@typescript-eslint/eslint-plugin": "^5.33.1",
    "@typescript-eslint/parser": "^5.33.1",
    "eslint": "8.22.0",
    "eslint-plugin-jsdoc": "^39.3.6",
    "mocha": "^10.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "ajv": "^8.11.0",
    "async-mqtt": "^2.6.2",
    "async-mutex": "^0.3.2",
    "axios": "^0.27.2",
    "compression": "^1.7.4",
    "debug": "^4.3.4",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "express-async-handler": "^1.2.0",
    "fs-extra": "^10.1.0",
    "html-entities": "^2.3.3",
    "http-status": "^1.5.2",
    "lodash": "^4.17.21",
    "selfsigned": "^2.0.1",
    "toad-scheduler": "^1.6.1"
  }
}
